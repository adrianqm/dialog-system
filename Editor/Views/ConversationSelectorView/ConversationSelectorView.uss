.group-ve {
    -unity-text-align: middle-left;
    flex-direction: row;
    min-height: 22px;
}

.group-label {
    -unity-text-align: middle-left;
    width: 100%;
    position: relative;
}

.hidden {
    display: none;
}

.group-textfield {
    width: 100%;
    padding-right: 5px;
    position: relative;
}

.unity-collection-view__item--selected {
    background-color: rgb(61, 61, 61);
}

.header-container {
    background-color: rgb(60, 60, 60);
    border-bottom-color: rgba(0, 0, 0, 0.4);
    border-bottom-width: 1px;
    height: 21px;
}

.plusElement {
    max-width: 17px;
}
